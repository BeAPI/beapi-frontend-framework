{
  "name": "beapi-frontend-framework",
  "version": "3.0.8",
  "author": "BeAPI",
  "repository": {
    "type": "git",
    "url": "https://github.com/BeAPI/beapi-frontend-framework"
  },
  "scripts": {
    "favicon": "node tasks/favicon.js && node tasks/favicon.js --appicon",
    "icons": "node tasks/svg-sprite.js",
    "composerjs": "cd composerjs && npm install && node app.js",
    "critical": "node tasks/critical-css.js",
    "bump": "node tasks/style-version.js",
    "start": "npm run icons && concurrently \"NODE_ENV=dev webpack --watch --config config/server.js\" \"php -S localhost:9090\" --kill-others",
    "dev": "NODE_ENV=dev webpack --watch --config config/webpack.dev.js",
    "prod": "webpack --config config/webpack.prod.js && npm run icons && npm run favicon && NODE_ENV=dev webpack --config config/webpack.dev.js",
    "build": "npm run prod && npm run critical && node tasks/style-version.js"
  },
  "license": "GPL-2.0",
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.55",
    "axe-core": "^3.0.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-sync": "^2.24.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cheerio": "^1.0.0-rc.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons": "^4.8.6",
    "file-loader": "^0.11.2",
    "fs": "0.0.1-security",
