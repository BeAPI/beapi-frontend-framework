{
  "name": "beapi-frontend-framework",
  "version": "4.0.0",
  "author": "BeAPI",
  "repository": {
    "type": "git",
    "url": "https://github.com/BeAPI/beapi-frontend-framework"
  },
  "scripts": {
    "start": "node_modules/.bin/concurrently -k -n \"Webpack,PHP Server\" -p \"[{name}]\" -c \"green, blue\" \"node_modules/.bin/webpack --mode=development --watch\" \"php -S localhost:9090\"",
    "build:dev": "node_modules/.bin/webpack --mode=development && npm run icon && npm run favicon",
    "build:prod": "node_modules/.bin/webpack --mode=production && npm run icon && npm run favicon",
    "build": "node_modules/.bin/webpack --mode=production && npm run build:dev",
    "watch": "node_modules/.bin/webpack --watch --mode=development",
    "bump": "node scripts/style-version.js",
    "composerjs": "cd composerjs && npm install && node app.js",
    "favicon": "node scripts/favicon.js && node scripts/favicon.js --appicon",
    "icon": "node scripts/svg-sprite.js",
    "image": "node scripts/image-sizes.js"
  },
  "license": "GPL-2.0",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-stage-2": "^6.24.1",
    "browser-sync": "^2.26.5",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^2.0.2",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "json2csv": "^4.5.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.1.3"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "cheerio": "^1.0.0-rc.3",
    "favicons": "^5.4.1",
    "lazysizes": "^5.0.0",
    "normalize-scss": "^7.0.1",
    "object-fit-images": "^3.2.4",
    "ora": "^3.4.0",
    "susy": "^2.2.14",
    "svgo": "^1.2.2",
    "svgstore": "^3.0.0-2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-progress-ora-plugin": "^1.2.1"
  }
}
