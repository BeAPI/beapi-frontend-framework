{
  "name": "beapi-frontend-framework",
  "version": "3.0.6",
  "author": "BeAPI",
  "repository": {
    "type": "git",
    "url": "https://github.com/BeAPI/beapi-frontend-framework"
  },
  "scripts": {
    "start": "concurrently \"NODE_ENV=dev webpack --watch --config config/server.js\" \"php -S localhost:9090\" --kill-others",
    "dev": "NODE_ENV=dev webpack --watch --config config/webpack.dev.js",
    "prod": "webpack --config config/webpack.prod.js && npm run favicon && NODE_ENV=dev webpack --config config/webpack.dev.js",
    "build": "npm run prod && node tasks/critical-css.js && node tasks/style-version.js && cd livingcss && npm install && gulp livingcss",
    "favicon": "node tasks/favicon.js && node tasks/favicon.js --appicon",
    "composerjs": "cd composerjs && npm install && node app.js",
    "bump": "node tasks/style-version.js",
    "livingcss": "cd livingcss && npm install && gulp livingcss && gulp sass"
  },
  "license": "GPL-2.0",
  "devDependencies": {
    "axe-core": "^3.0.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-sync": "^2.4.0",
    "browser-sync-webpack-plugin": "^1.2.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.4",
    "css-mqpacker": "^6.0.2",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons": "^4.8.6",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^3.2.0",
